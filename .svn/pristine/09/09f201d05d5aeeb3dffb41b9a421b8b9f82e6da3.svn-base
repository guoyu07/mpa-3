@import '../../styles/fonts/iconfont';
*{
    margin: 0;
    padding: 0;
    font-size: 14px;
    text-decoration: none;
    line-height: 1.42857;
}
.m-leftnav{
  ul{
    list-style: none;
    }
    width: 140px;
    float:left;
    text-align: left;
    padding:0px 30px 10px 30px;
    background: #fff;
    margin-bottom:30px;
    .m-leftnav-con{
        margin-top: 13px;
        .m-leftnav-item{
            border-bottom: 1px solid #DDDDDD;
            h3{
                font-size: 14px;
                i{
                    font-weight:500;
                    color:#999;
                    padding: 10px;
                    a{
                    color:#333;
                    &:hover{
                        color:#000;
                     }
                }
                }

            }
            ul{
               li{
                    padding-left: 34px;
                    cursor:pointer;
                    color:#999;
                    margin: 17px 0px;
                    a{
                        color:#333;
                        &:hover{
                            color:#4565F7 ;
                        }
                    }
                    .arrow{
                        margin-top: 9px;
                        border:5px solid;
                        border-color:#000 transparent transparent transparent;
                        display: inline-block;
                        transition: transform 0.5s;
                        float: right;
                    }
                    .trans{
                        transform: rotate(180deg);
                        -ms-transform: rotate(180deg); /* IE 9 */
                        -webkit-transform: rotate(180deg);
                        transform-origin:20% 20%;
                        -ms-transform-origin:20% 20%; /* IE 9 */
                        -webkit-transform-origin:20% 20%; /* Safari and Chrome */
                    }
                     li{
                         padding-left: 15px;
                    }
                }
            }
        }

    }
}
