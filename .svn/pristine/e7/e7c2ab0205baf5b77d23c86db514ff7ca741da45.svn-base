.m-leftnav{
    width: 200px;
    float:left;
    text-align: left;
    padding:0px 30px 10px 30px;
    background: #fff;
    margin-bottom:30px;
    .m-leftnav-con{
        .m-leftnav-item{
            border-bottom: 1px solid #DDDDDD;
            h3{
                i{
                    font-weight:500;
                    color:#999;
                    padding: 10px;
                }
                a{

                    color:#333;
                    &:hover{
                        color:#000;
                    }
                }
            }
            ul{
               li{
                    padding-left: 34px;
                    cursor:pointer;
                    color:#999;
                    margin: 17px 0px;
                    a{
                        color:#333;
                        &:hover{
                            color:#000;
                        }
                    }
                    .arrow{
                        margin-top: 9px;
                        border:5px solid;
                        border-color:#000 transparent transparent transparent;
                        display: inline-block;
                        transition: transform 0.5s;
                    }
                    .trans{
                        transform: rotate(180deg);
                        -ms-transform: rotate(180deg); /* IE 9 */
                        -webkit-transform: rotate(180deg);
                        transform-origin:20% 20%;
                        -ms-transform-origin:20% 20%; /* IE 9 */
                        -webkit-transform-origin:20% 20%; /* Safari and Chrome */
                    }
                     li{
                         padding-left: 15px;
                    }
                } 
            }
        }

    }
}