@import "./reset";
.clearfix() {
    zoom: 1;
    &:before, &:after {
        display: table;
        content: " ";
    }
    &:after {
        clear: both;
    }
}

.g-body {
  position: relative;
  background: #f5f5f5;
}
@boxWidth: 1190px;
.g-box {
  width: @boxWidth;
  height: 100%;
  position: relative;
  margin: 0 auto;
  .clearfix();
}

.m-topbar {
  @height: 30px;
  z-index: 99;
  height: @height;
  font-size:12px;
  a {
    color: #252525;
  }
  ul > li {
    line-height: @height;
    float: left;
    display: inline-block;

    + li {
      margin-left: 25px;
    }
  }
  .m-topbar-logo {
    display: inline-block;
    background: url("/style/img/logo.png") center center no-repeat;
    width: 140px;
    height: 41px;
    overflow: hidden;
    margin-top: 7px;
  }
}

.f-float-right {
    float: right;
}
