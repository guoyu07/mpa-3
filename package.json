    "bootstrap": "^3.3.7",
    "bootstrap-datetime-picker": "^2.4.4",
    "handlebars": "^4.0.11",
    "handlebars-layouts": "^3.1.4",
    "handlebars-loader": "^1.6.0",
    "html2canvas": "^0.5.0-beta4",
    "jquery": "^3.2.1",
    "jspdf": "^1.3.5",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "sass-loader": "^6.0.6",
    "tinymce": "^4.7.2"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.17",
